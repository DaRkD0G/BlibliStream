<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#D9ffffff"></solid>

            <stroke android:width="2dip" android:color="#D9ffffff" />
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />

        </shape>
    </item>


    <item android:state_focused="true">
        <shape>
            <solid android:color="#D9ffffff"></solid>
            <stroke android:width="2dip" android:color="#D9ffffff" />
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="@android:color/white"></solid>
            <stroke android:width="2dip" android:color="#D9ffffff" />
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />


        </shape>

    </item>

</selector>